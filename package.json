{
  "name": "air-supply",
  "version": "0.0.4",
  "description": "You need data; Air Supply will get it to you.",
  "bin": {
    "air-supply": "bin/air-supply"
  },
  "main": "index.js",
  "files": [
    "dist/**/*",
    "src/**/*",
    "bin/**/*",
    "README.md",
    "index.*"
  ],
  "scripts": {
    "test": "jest",
    "docs": "mkdir -p docs && documentation build -f html -c documentation.yml --github -o docs index.js",
    "docs:preview": "documentation serve -c documentation.yml --github -w src/**/* index.js",
    "docs:publish": "npm run docs && gh-pages -d docs",
    "prepublishOnly": "npm run test"
  },
  "author": "Alan Palazzolo",
  "license": "MIT",
  "devDependencies": {
    "documentation": "^9.1.1",
    "dotenv": "^6.2.0",
    "eslint": "^5.11.1",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "sqlite3": "^4.0.6",
    "archieml": "^0.4.2",
    "csv-parse": "^4.3.1",
    "@mapbox/togeojson": "^0.16.0",
    "reproject": "^1.2.3",
    "epsg": "^0.5.0",
    "shpjs": "^3.4.3",
    "topojson": "^3.0.2",
    "xlsx": "^0.14.1",
    "js-yaml": "^3.12.1"
  },
  "dependencies": {
    "adm-zip": "^0.4.13",
    "airtable": "^0.5.8",
    "cosmiconfig": "^5.0.7",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "ftp": "^0.3.10",
    "glob": "^7.1.3",
    "googleapis": "^37.1.0",
    "html-entities": "^1.2.1",
    "htmlparser2": "^3.10.0",
    "json5": "^2.1.0",
    "killable": "^1.0.1",
    "lodash": "^4.17.11",
    "node-fetch": "^2.3.0",
    "object-hash": "^1.3.1",
    "opn": "^5.4.0",
    "sequelize": "^4.42.0",
    "xmldom": "^0.1.27",
    "yargs": "^13.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zzolo/air-supply.git"
  },
  "bugs": {
